{
	"extends": "stylelint-config-standard",
	"rules": {
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": ["--my-at-rule"]
			}
		],
		"indentation": "tab",
		"number-leading-zero": null,
		"number-max-precision": [
			4,
			{
				"severity": "warning"
			}
		],
		"unit-allowed-list": ["em", "rem", "s"]
	}
}
